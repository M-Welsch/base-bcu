{
    "ssh_host": {
        "type": "str",
        "optional": true,
        "regex": "(\\b25[0-5]|\\b2[0-4][0-9]|\\b[01]?[0-9][0-9]?)(\\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)){3}"},
    "ssh_user": {
        "type": "str",
        "optional": true,
        "regex": "^[a-z_]([a-z0-9_-]{0,31}|[a-z0-9_-]{0,30}\\$)$"
    },
    "logfile_target_path": {
        "type": "pathlib.Path",
        "optional": false
    }
}